{
    "name": "gipfl/protocol-snmp",
    "description": "SNMP protocol implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "gipfl\\Protocol\\Snmp\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gipfl/protocol",
            "no-api":true
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-json": "*",
        "gipfl/protocol": ">=0.2",
        "react/datagram": ">=1.4",
        "sop/asn1": "^4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
